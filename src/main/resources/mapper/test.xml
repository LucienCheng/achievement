<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper SYSTEM "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.dao.TestDao">
<resultMap type="com.entity.Test" id="test">
<id property="Id" column="userId"/>
<collection property="test" javaType="ArrayList" ofType="int" >
<result column="audId"/>
</collection>
</resultMap>
	<select id="searchTest" resultMap="test">
	select `user`.* ,audit.audId from `user`,audit
WHERE `user`.userId=audit.userId
	</select>

</mapper>