<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper SYSTEM "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.dao.AlterMapper">
	<!-- 插入一条更改记录 -->
	<insert id="insertAchiAlter" >
		insert into
		alternative(oldId,newId)
		values(#{oldId},#{newId})
	</insert>
	<!-- 删除一条更改记录 -->
	<delete id="deleteAchiAlter">
		delete from alternative
		where newId=#{newId}
	</delete>
	<select id="selectAlter" resultType="int">
	select alternative.oldId from
	alternative
	<where>
	newId=#{newId}
	</where>
	
	</select>

</mapper>